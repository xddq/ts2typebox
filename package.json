{
  "name": "ts2typebox",
  "version": "1.0.8",
  "description": "Create typebox types based on typescript types",
  "main": "dist/src/index.js",
  "source": "dist/src/index.js",
  "bin": {
    "ts2typebox": "dist/src/index.js",
    "ts-to-typebox": "dist/src/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch-ts": "tsc -w",
