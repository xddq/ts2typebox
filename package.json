    "semver-minor": "npm version minor -f",
    "publish": "npm publish",
    "start": "node dist/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xddq/ts2typebox.git"
  },
  "keywords": [
    "ts2typebox",
    "ts-to-typebox",
    "typescript",
    "typebox",
    "ts",
    "validation",
    "parsing",
    "IO",
    "cli"
  ],
  "author": "Pierre Dahmani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xddq/ts2typebox/issues"
  },
  "homepage": "https://github.com/xddq/ts2typebox#readme",
  "dependencies": {
    "@sinclair/typebox": "0.28.9",
    "minimist": "1.2.8",
    "prettier": "2.8.7",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.2",
    "@types/node": "18.15.3",
    "@types/prettier": "^2.7.2",
    "tsconfig-paths": "4.1.2"
  }
}
